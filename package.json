{
  "name": "@BuildHero/watermelondb",
  "description": "Build powerful React Native and React web apps that scale from hundreds to tens of thousands of records and remain fast",
  "version": "5.0.0-40",
  "scripts": {
    "build": "NODE_ENV=production node ./scripts/make.js",
    "dev": "NODE_ENV=development node ./scripts/make.js",
    "dev:native": "react-native start",
    "release": "node ./scripts/publish.js",
    "prettier": "prettier-eslint --config ./.prettierrc --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "eslint": "eslint ./src -c ./.eslintrc.yml --cache --cache-location ./.cache/.eslintcache --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "jest --config=./jest.config.js --forceExit --detectOpenHandles --no-cache",
    "test:watch": "jest --config=./jest.config.js --watch",
    "test:coverage": "jest --config=./jest.config.js --coverage",
    "ci:check": "concurrently 'npm run test' 'npm run eslint' 'npm run typecheck' --kill-others-on-fail",
    "test:android": "cd native/androidTest; ./gradlew connectedAndroidTest",
    "test:ios": "cd native/iosTest; xcodebuild -workspace WatermelonTester.xcworkspace -scheme 'WatermelonTester' -destination 'name=iPhone 8' test",
    "test:typescript": "cd examples/typescript; yarn; npm run test",
    "swiftlint": "cd native; swiftlint",
    "ktlint": "cd native/androidTest; ./gradlew ktlint",
    "ktlint:format": "cd native/androidTest; ./gradlew ktlintFormat",
    "android:emulator": "./scripts/emulatorWithJavaCheck",
    "docs": "./scripts/mdbook"
  },
  "codegenConfig": {
    "name": "WatermelonDBSpec",
    "type": "all",
    "jsSrcsDir": "native/specs",
    "android": {
      "javaPackageName": "com.watermelondb.specs"
    }
  },
  "author": "@BuildHero",
  "homepage": "https://github.com/BuildHero/WatermelonDB#readme",
  "bugs": "https://github.com/BuildHero/WatermelonDB/issues",
  "license": "MIT",
  "keywords": [
    "database",
    "indexeddb",
    "sqlite",
    "react",
    "react-native",
    "watermelon",
    "offline",
    "persistence"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildHero/WatermelonDB.git"
